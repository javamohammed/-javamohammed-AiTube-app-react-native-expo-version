{
  "expo": {
    "name": "AichaTube",
    "description":"This Application for kids",
    "slug": "ai-tube-app",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android":{
      "package":"com.mido.aitube",
      "versionCode":1,
      "adaptiveIcon":{
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions":[
        "CAMERA", "READ_EXTERNAL_STORAGE"
      ]
    }
  }
}